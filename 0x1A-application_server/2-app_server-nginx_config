server {
    listen 80 default_server;
    server_name _;
    add_header  X-Served-By $hostname;
    root /etc/nginx/html;
    index index.html index.htm;
    root /var/www/html;
 
    location /airbnb-onepage {
        proxy_pass http://127.0.0.1:5000/airbnb-onepage;
    }
    
    location /{
        try_files $uri $uri/ =404;
     }
}
